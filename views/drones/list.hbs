<h2>Available drones</h2>
{{#each drones}}
<div>
<p>Name:{{name}}</p>
<p>Amount of propellers: {{propellers}}</p>
<p>Speed:{{maxSpeed}}</p>
<a href="/drones/{{_id}}/edit">Edit</a>
<form action="/drones/{{id}}/delete" method="post">
<button type="submit">Delete Drone</button></form>
<hr>
</div>
{{/each}}

